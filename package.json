{
  "name": "eslint-config-auto",
  "version": "0.3.4",
  "author": "David J. Bradshaw",
  "description": "Automatically configure esLint based on project dependancies",
  "repository": {
    "type": "git",
    "url": "https://github.com/davidjbradshaw/eslint-config-auto.git"
  },
  "main": "index.js",
  "scripts": {
    "ci-test": "npm run test",
    "eslint": "eslint --color  *.{js,json,md} rules/**.js lib/**.js",
    "eslint:fix": "eslint --color --fix  *.{js,md} rules/**.js lib/**.js",
    "prepublishOnly": "eslint-find-rules --unused --no-core",
    "test": "npm run eslint && npm run prepublishOnly"
  },
  "keywords": [
    "airbnb",
    "config",
    "eslint",
    "eslintconfig",
    "javascript",
    "smartpension"
  ],
  "dependencies": {
    "@typescript-eslint/parser": "^3.2.0",
    "arrify": "^2.0.1",
    "babel-eslint": "^10.1.0",
    "cosmiconfig": "^6.0.0",
    "eslint-config-adjunct": "latest",
    "eslint-config-airbnb-base": "^14.2.0",
    "lodash": "^4.17.15",
    "mkdir": "0.0.2",
    "mkdirp": "^1.0.4",
    "ramda": "^0.27.0",
    "read-pkg-up": "^7.0.1",
    "rimraf": "^3.0.2"
  },
  "devDependencies": {
    "eslint": "^7.2.0",
    "eslint-find-rules": "^3.5.0",
    "husky": "^4.2.5",
    "in-publish": "^2.0.1",
    "prettier": "^2.0.5",
    "prettier-cli": "^0.1.0",
    "react": "^16.13.1",
    "safe-publish-latest": "^1.1.4"
  },
  "engines": {
    "node": ">= 8.3"
  }
}
